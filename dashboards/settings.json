color='hotpink'\u003eTask wird ausgef√ºhrt\u003c/font\u003e\u003cp\u003e\u003cbr\u003eBitte warten...\";\r\n        $(\"#settings_output\").html(input_payload);\r\n\r\n        var dt = new Date();\r\n        curdate = (dt.getFullYear()) + ((\"0\"+(dt.getMonth()+1)).slice(-2)) + ((\"0\"+dt.getDate()).slice(-2)) + \" / \" + ((\"0\"+dt.getHours()).slice(-2)) +\":\"+ ((\"0\"+dt.getMinutes()).slice(-2)) +\":\"+ ((\"0\"+dt.getSeconds()+1).slice(-2));\r\n\r\n        var cmnd = \"systemtools '\" + param + \"' '\" + curdate + \"'\";\r\n        publish_settings(cmnd);\r\n        \r\n    }\r\n\r\n\u003c/script\u003e\r\n","mode":"html"},"pluginVersion":"8.0.6","timeFrom":null,"timeShift":null,"type":"text"},{"datasource":null,"gridPos":{"h":4,"w":6,"x":0,"y":31},"id":22,"options":{"content":"\u003ch4\u003eSystem Input\u003c/h4\u003e\r\n\u003chr /\u003e\r\n\r\n\u003cinput title=\"Linux Befehle ausf√ºhren\" placeholder=\"enter non-interactive bash commands\" class=\"nanohome\" type=\"text\" id=\"input\" value=\"Input\" onfocus=\"if (this.value === 'Input') { this.value=''; } \" autocomplete=\"off\"/\u003e\r\n\r\n\u003cscript\u003e\r\n$(\"input\").on(\"keydown\",function search(e) {\r\n    if(e.keyCode == 13) {\r\n      \r\n        var inp = document.getElementById(\"input\").value;\r\n        \r\n        if (inp != \"\") {\r\n            publish_settings(inp);\r\n          } else {\r\n            publish_settings(\"nanohelp\");\r\n          }\r\n    }\r\n});\r\n\u003c/script\u003e\r\n","mode":"html"},"pluginVersion":"8.0.6","targets":[{"groupBy":[{"params":["$__interval"],"type":"time"},{"params":["null"],"type":"fill"}],"orderByTime":"ASC","policy":"default","refId":"A","resultFormat":"time_series","select":[[{"params":["value"],"type":"field"},{"params":[],"type":"mean"}]],"tags":[]}],"timeFrom":null,"timeShift":null,"type":"text"}],"schemaVersion":30,"style":"dark","tags":[],"templating":{"list":[]},"time":{"from":"now-6h","to":"now"},"timepicker":{"hidden":true,"refresh_inter